{% extends "ICAffichageBundle::layout.html.twig" %}

{% block title %}{{ parent() }} - Produit Idcapt{% endblock %}

{% block entete %}{{ parent() }} Produit Idcapt{% endblock %}

{% block stylesheets %}
	{% stylesheets filter='cssrewrite'
	'@ICAffichageBundle/Resources/public/css/sous_traitant.css' %}
		{{ parent() }}
		<link rel="stylesheet" href="{{ asset_url }}" type="text/css">
	{% endstylesheets %}
{% endblock %}

{% block body %}
{{ parent() }}
	<h1>Stock Lecteur {{page}}</h1>
	<table>
		<tr>
			<th>Référence</th>
			<th>Désignation</th>
			<th>Quantité</th>
		</tr>
		{% for lect in lecteur %}
		<tr>
			<td>{{lect.referenceInterne}}</td>
			<td>{{lect.designation}}</td>
			<td>{{lect.nbProduit}}</td>
		</tr>
		{% endfor %}
	</table>
{% endblock %}